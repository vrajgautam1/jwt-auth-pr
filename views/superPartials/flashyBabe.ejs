<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>

    <% if(error.length){ %>
        <script>
            swal({
                title: "Error",
                text: "<%= error %>",
                icon: "error",
            });
        </script>
        <% } %>

            <% if(success.length){ %>
                <script>
                    swal({
                        title: "<%= success %>",
                        icon: "success",
                    });
                </script>
                <% } %>